{
  "expo": {
    "name": "Advantage Home",
    "slug": "advantage-home-contracting",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A5872"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.advantagehandy.advantagehome",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.694885915476-9oqmlpj23bf3db8c7sg8hmiiqubbbcnr"
        }
      },
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take a picture of a home improvement problem for a free estimate.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to upload an existing photo for a free estimate."
      }
    }
  }
}